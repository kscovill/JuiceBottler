      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_RnogNKpvEee0e-homK-IiA" name="processOrange" specification="_Rk1D0KpvEee0e-homK-IiA">
        <language>java</language>
        <body>Orange o = in.getOrange();&#xD;
		if (o.getState() == Orange.State.Bottled) {&#xD;
			o.runProcess();&#xD;
			Plant.orangesProcessed[0]++;&#xD;
&#xD;
		}&#xD;
	</body>
