      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_RnogMapvEee0e-homK-IiA" name="peelOrange" specification="_Rkz1sqpvEee0e-homK-IiA">
        <language>java</language>
        <body>Orange o = in.getOrange();&#xD;
		if (o.getState() == Orange.State.Fetched) {&#xD;
			o.runProcess();&#xD;
			out.addOrange(o);&#xD;
		}&#xD;
	</body>
      </ownedBehavior>
