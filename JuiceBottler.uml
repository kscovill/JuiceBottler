        <language>java</language>
        <body>//System.out.println(&quot;The &quot; + Thread.currentThread().getName() + &quot; is Processing oranges&quot;);&#xD;
		while (timeToWork) {&#xD;
			if (role == 0) {&#xD;
				fetchOrange();&#xD;
			} else if (role == 1) {&#xD;
				peelOrange();&#xD;
			} else if (role == 2) {&#xD;
				squeezeOrange();&#xD;
			} else if (role == 3) {&#xD;
				bottleOrange();&#xD;
			} else if (role == 4) {&#xD;
				processOrange();&#xD;
			}&#xD;
		}&#xD;
	</body>
      </ownedBehavior>
